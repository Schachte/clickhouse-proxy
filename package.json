
{
  "name": "cloudflare-tunnel-manager",
  "version": "1.0.0",
  "description": "Manages Cloudflare tunnels",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc && pkg . --output build/clickhouse-proxy",
    "setup": "bash ./setup.sh",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "clickhouse-proxy": "./dist/index.js"
  },
  "pkg": {
    "targets": [
      "node18-macos-x64"
    ],
    "outputPath": "bin"
  },
  "dependencies": {
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.17",
    "nodemon": "^3.0.3",
    "pkg": "^5.8.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}